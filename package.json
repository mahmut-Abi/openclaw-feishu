{
  "name": "@mahmut-abi/feishu",
  "version": "0.1.7",
  "type": "module",
  "description": "OpenClaw Feishu/Lark channel plugin",
  "license": "MIT",
  "files": [
    "index.ts",
    "src",
    "openclaw.plugin.json"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/mahmut-Abi/openclaw-feishu.git"
  },
  "homepage": "https://github.com/mahmut-Abi/openclaw-feishu#readme",
  "bugs": {
    "url": "https://github.com/mahmut-Abi/openclaw-feishu/issues"
  },
  "keywords": [
    "openclaw",
    "feishu",
    "lark",
    "飞书",
    "chatbot",
    "ai",
    "claude"
  ],
  "openclaw": {
    "extensions": [
      "./index.ts"
    ],
    "channel": {
      "id": "feishu",
      "label": "Feishu",
      "selectionLabel": "Feishu/Lark (飞书)",
      "docsPath": "/channels/feishu",
      "docsLabel": "feishu",
      "blurb": "飞书/Lark enterprise messaging.",
      "aliases": [
        "lark"
      ],
      "order": 70
    },
    "install": {
      "npmSpec": "@mahmut-abi/feishu",
      "localPath": ".",
      "defaultChoice": "npm"
    }
  },
  "dependencies": {
    "@larksuiteoapi/node-sdk": "^1.30.0",
    "remark": "^15.0.1",
    "remark-gfm": "^4.0.1",
    "remark-parse": "^11.0.0",
    "remark-stringify": "^11.0.0",
    "unified": "^11.0.5",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@types/mdast": "^4.0.4",
    "@types/node": "^25.0.10",
    "openclaw": "2026.1.29",
    "tsx": "^4.21.0",
    "typescript": "^5.7.0"
  },
  "peerDependencies": {
    "openclaw": ">=2026.1.29"
  },
  "publishConfig": {
    "access": "public"
  }
}
